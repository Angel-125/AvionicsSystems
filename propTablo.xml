		<prop>
			<name>MAS_Tablo_Alarm_Ablation</name>
			<style>Alarm</style>
			<TEXT_LABEL>
				<text>ABLATION</text>
				<variable>fc.Conditioned(fc.ResourceDelta("Ablator") &gt; 0.1)</variable>
			</TEXT_LABEL>
			<COLOR_SHIFT>
				<variable>fc.Conditioned(fc.ResourceDelta("Ablator") &gt; 0.1)</variable>
			</COLOR_SHIFT>
