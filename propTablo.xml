				<variable>fc.Conditioned(fc.TargetDistance() &lt; 10 and fc.TargetSpeed() &gt; 2.5)</variable>
			</TRIGGER_EVENT>
			<TEXT_LABEL>
				<text>SPEED</text>
				<fontSize>5</fontSize>
				<variable>fc.Conditioned(fc.TargetDistance() &lt; 10 and fc.TargetSpeed() &gt; 2.5)</variable>
			</TEXT_LABEL>
