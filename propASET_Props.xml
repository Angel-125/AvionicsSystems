	<LightSwitch folder="LightSwitch">
		<style name="Push Button">
			<MODEL>
				<model>ASET/ASET_Props/Control/LightSwitch/SwitchLightButton</model>
			</MODEL>
			<COLLIDER_EVENT id="0">
				<name>Collider</name>
				<collider>SwitchPUSHcollider</collider>
				<sound>ASET/ASET_Props/Sounds/buttonbeep</sound>
				<volume>0.5</volume>
			</COLLIDER_EVENT>
			<COLOR_SHIFT id="0">
				<name>Label Illumination</name>
				<transform>SwitchNamePlate</transform>
				<variable>fc.Conditioned(fc.GetPersistentAsNumber("Backlight"))</variable>
				<passiveColor>0,0,0,255</passiveColor>
				<activeColor>COLOR_ASET_PUSHBUTTON_BACKLIGHT_ACTIVECOLOR</activeColor>
				<blend>true</blend>
			</COLOR_SHIFT>
			<COLOR_SHIFT id="1">
				<name>Border Illumination</name>
				<transform>buttonBorder</transform>
				<passiveColor>0,0,0,255</passiveColor>
				<activeColor>COLOR_ASET_STANDART_PUSHBUTTON_ACTIVECOLOR</activeColor>
			</COLOR_SHIFT>
			<ANIMATION_PLAYER>
				<name>Switch animation</name>
				<animation>SwitchLightAnim</animation>
				<animationSpeed>1.0</animationSpeed>
			</ANIMATION_PLAYER>
			<TEXTURE_SHIFT id="0">
				<name>Label</name>
				<transform>SwitchNamePlate</transform>
				<layers>_MainTex _Emissive</layers>
			</TEXTURE_SHIFT>
		</style>
		<prop>
			<name>MAS_AirLockLightSwitch</name>
			<style>Push Button</style>
			<COLLIDER_EVENT id="0">
				<onClick>fc.TogglePersistent("AirLockLights")</onClick>
			</COLLIDER_EVENT>
			<COLOR_SHIFT id="1">
				<activeColor>130,60,20,255</activeColor>
				<variable>fc.Conditioned(fc.GetPersistentAsNumber("AirLockLights"))</variable>
			</COLOR_SHIFT>
			<ANIMATION_PLAYER id="0">
				<variable>fc.GetPersistentAsNumber("AirLockLights")</variable>
			</ANIMATION_PLAYER>
			<TEXTURE_SHIFT id="0">
				<startUV>0.25, 0.50</startUV>
			</TEXTURE_SHIFT>
			<ANIMATION_PLAYER id="1">
				<name>External Airlock Lights</name>
				<externalAnimation>AirLockLightsAnim</externalAnimation>
				<variable>fc.GetPersistentAsNumber("AirLockLights")</variable>
			</ANIMATION_PLAYER>
		</prop>
